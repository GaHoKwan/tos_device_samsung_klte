        <!-- Video Software  -->
        <MediaCodec name="OMX.SEC.h263.sw.dec" type="video/3gpp" >
            <Quirk name="decoder-ignores-streamcorrupt-error" />	
        </MediaCodec>
        <MediaCodec name="OMX.SEC.avc.sw.dec" type="video/avc" >
            <Quirk name="decoder-ignores-streamcorrupt-error" />	
