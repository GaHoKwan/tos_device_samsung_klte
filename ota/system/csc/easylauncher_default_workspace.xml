<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.easylauncher" >

	<home>
        <appwidget
            launcher:className="com.sec.android.app.shealth.widget.WalkMateAppEasyWidget"
            launcher:spanX="2"
            launcher:iconPosition="100"
            launcher:packageName="com.sec.android.app.shealth"
            launcher:screen="-1" />

		<!--
              appwidget : contact widget, weather widget and calendar widget are fixed
              this is changed only when packageName or className is changed.
		-->
		<!-- Screen 0 -->
		<appwidget
			launcher:className="com.sec.android.widgetapp.easymodecontactswidget.SeniorFavoriteWidgetProviderLarge"
			launcher:iconPosition="0"
			launcher:packageName="com.sec.android.widgetapp.easymodecontactswidget"
			launcher:screen="0" />

		<!-- Screen 1 -->
		<appwidget
			launcher:className="com.sec.android.widgetapp.easymodecontactswidget.SeniorFavoriteWidgetProviderLarge"
			launcher:iconPosition="0"
			launcher:packageName="com.sec.android.widgetapp.easymodecontactswidget"
			launcher:screen="1" />
			
		<!-- Screen 2 -->
		<!--
              appshortcut : app shortcut refers to application shortcuts on easy launcher home page 
              we can replace the default shortcuts in main screen and favorite apps page by changing the shortcuts information here.
		-->
		<!-- In case of screen 1 position 0 to 5 are occupied by application shortcuts. position above 6 are reserved for application widgets -->

		<appshortcut
			launcher:className="com.android.settings.accessibilitywidget.AccessibilityEasyWidgetProviderAssistiveLight"
			launcher:iconPosition="0"
			launcher:packageName="com.android.settings"
			launcher:screen="2" />
		<appshortcut
			launcher:className="com.sec.android.app.magnifier.Magnifier"
			launcher:iconPosition="1"
			launcher:packageName="com.sec.android.app.magnifier"
			launcher:screen="2" />
		<appshortcut
			launcher:className="com.sec.android.app.camera.Camera"
			launcher:iconPosition="2"
			launcher:packageName="com.sec.android.app.camera"
			launcher:screen="2" />
		<appshortcut
			launcher:className="com.android.dialer.DialtactsActivity"
			launcher:iconPosition="3"
			launcher:packageName="com.android.contacts"
			launcher:screen="2" />
		<appshortcut
			launcher:className="com.android.mms.ui.ConversationComposer"
			launcher:iconPosition="4"
			launcher:packageName="com.android.mms"
			launcher:screen="2" />
		<appshortcut
			launcher:className="com.sec.android.app.sbrowser.SBrowserMainActivity"
			launcher:iconPosition="5"
			launcher:packageName="com.sec.android.app.sbrowser"
			launcher:screen="2" />

		<!--
             Screen 2 refers to center screen
            Widgets in center screen: Weather widget (position 6)  Splanner widget( position 7)
		-->
        <appwidget
            launcher:className="com.sec.android.widgetapp.ap.hero.cmaweather.easy.widget.WeatherClock"
            launcher:iconPosition="100"
            launcher:spanX="2"
            launcher:packageName="com.sec.android.widgetapp.ap.hero.cmaweather"
            launcher:screen="2" />
            
		<appwidget
			launcher:className="com.sec.android.widgetapp.digitalclockeasy.DigitalClockEasyWidgetProvider"
			launcher:iconPosition="101"
            launcher:spanX="1"
			launcher:packageName="com.sec.android.widgetapp.digitalclockeasy"
			launcher:screen="2" />
		<appwidget
			launcher:className="com.sec.android.widgetapp.SPlannerAppWidget.EasyWidget.EasyCalendarAppWidgetProvider"
			launcher:iconPosition="102"
            launcher:spanX="1"
			launcher:packageName="com.sec.android.widgetapp.SPlannerAppWidget"
			launcher:screen="2" />

		<appwidget
			launcher:className="com.android.settings.accessibilitywidget.AccessibilityEasyWidgetProviderAssistiveLight"
			launcher:iconPosition="103"
            launcher:spanX="1"
			launcher:packageName="com.android.settings"
			launcher:screen="2" />
        <!--
       Screen 3 refers to favorite apps screen
           All the default applications shortcuts will be defined here


        
        <appwidget
            launcher:className="com.sec.android.app.shealth.widget.homescreenwidget.WalkMateAppWidget"
            launcher:spanX="2"
            launcher:iconPosition="103"
            launcher:packageName="com.sec.android.app.shealth"
            launcher:screen="2" />
        -->

		<!-- Screen 3 -->
		<appshortcut
			launcher:className="com.sec.android.gallery3d.app.GalleryActivity"
			launcher:iconPosition="0"
			launcher:packageName="com.sec.android.gallery3d"
			launcher:screen="3" />		
		<appshortcut
			launcher:className="com.android.email.activity.Welcome"
			launcher:iconPosition="1"
			launcher:packageName="com.android.email"
			launcher:screen="3" />
		<appshortcut
			launcher:className="com.android.calendar.AllInOneActivity"
			launcher:iconPosition="2"
			launcher:packageName="com.android.calendar"
			launcher:screen="3" />
		<appshortcut
			launcher:className="com.sec.android.app.myfiles.MainActivity"
			launcher:iconPosition="3"
			launcher:packageName="com.sec.android.app.myfiles"
			launcher:screen="3" />	
		<appshortcut
			launcher:className="com.samsung.musicplus.MusicMainActivity"
			launcher:iconPosition="4"
			launcher:packageName="com.sec.android.app.music"
			launcher:screen="3" />			
		<appshortcut
			launcher:className="com.sec.android.app.samsungapps.SamsungAppsMainActivity"
			launcher:iconPosition="5"
			launcher:packageName="com.sec.android.app.samsungapps"
			launcher:screen="3" />
		<appshortcut
			launcher:className="com.sec.android.easysettings.EasySettingsHomeActivity"
			launcher:iconPosition="6"
			launcher:packageName="com.sec.android.easysettings"
			launcher:screen="3" />

			
										<!-- Screen for apps help page -->
		<appshortcut
			launcher:className="com.android.email.activity.Welcome"
			launcher:iconPosition="0"
			launcher:packageName="com.android.email"
			launcher:screen="-1" />
		<appshortcut
			launcher:className="com.android.calendar.AllInOneActivity"
			launcher:iconPosition="1"
			launcher:packageName="com.android.calendar"
			launcher:screen="-1" />
			
		<!-- Screen for contacts help page -->
		<appwidget
			launcher:className="com.sec.android.widgetapp.easymodecontactswidget.SeniorFavoriteWidgetProviderLarge"
			launcher:iconPosition="0"
			launcher:packageName="com.sec.android.widgetapp.easymodecontactswidget"
			launcher:screen="-2" />
			
		<!--
             There are always some applications which are required to be shown in easy mode and not in standard mode and vice-versa.
            So, we have created this tag <easyapps> which contains class name and package name and a flag which says whether the app should be visible.
            If the visibility flag is true, this app will be shown in easy launcher, if false, this app will not be shown in easy launcher
		-->
		<!-- Declare all the Easy apps which are to be shown and laucnhed only from EasyLauncher -->
		<easyapps
			launcher:className="com.sec.android.easysettings.EasySettingsHomeActivity"
			launcher:packageName="com.sec.android.easysettings"
			launcher:visibility="true" />
        <easyapps
            launcher:className="com.sec.android.app.magnifier.Magnifier"
			launcher:packageName="com.sec.android.app.magnifier"
			launcher:visibility="true" />
        <easyapps
            launcher:className="com.samsung.android.app.galaxyfinder.GalaxyFinderActivity"
            launcher:packageName="com.samsung.android.app.galaxyfinder"
            launcher:visibility="false" />		
	</home>

</favorites>
